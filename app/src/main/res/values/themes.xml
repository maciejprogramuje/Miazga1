    <style name="Theme.Miazga1.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.Miazga1.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>